{:system/env #profile {:dev :dev
                       :prod :prod}

 :http/server
 {:port #long #or [#env VENN_AGENT_PORT 8080]
  :host #or [#env VENN_AGENT_HOST "0.0.0.0"]}


 :db/server
 {:dbtype "postgresql"
  :dbname #or [#env VENN_AGENT_DB_NAME "venn_agent_development"]
  :host #or [#env VENN_AGENT_DB_HOST :none]
  :port #or [#env VENN_AGENT_DB_PORT :none]
  :user #or [#env VENN_AGENT_DB_USER ""]
  :password #or [#env VENN_AGENT_DB_PASSWORD "password"]}}
